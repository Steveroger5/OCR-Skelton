<%- include ("partials/header") %>

<div class="container">
<h1 class="text-center">
	Form <i class="book icon"></i>
</h1>
	<div class="well p-3 mb-2 jumbotron text-dark">
	<div style="width:30%; margin: 70px auto;">
	<div class="form-group"><strong>Name</strong><i class="arrow right icon"></i><%= form.name %>
	</div>
	<div class="form-group"><strong>Rollno</strong><i class="arrow right icon"></i><%= form.rollno %>
	</div>
	<div class="form-group"><strong>Department</strong><i class="arrow right icon"></i><%= form.dept %>
	</div>
	<div class="form-group">
		<a href="/myform/<%= form._id %>/edit" 
			class="btn btn-sm btn-warning">Edit <i class="pencil icon"></i></a>
					
		<form action="/myform/<%= form._id %>?_method=DELETE" method="POST" style="display: inline">
			<button class="btn btn-sm btn-danger">Delete <i class="delete icon"></i></button>
		</form>
		</div>
		<a href="/view %>">Go Back</a>
	</div>	
	</div>	
</div>
<%- include ("partials/footer") %>